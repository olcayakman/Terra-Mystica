<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane onKeyPressed="#handle" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.GameplayController">
   <children>
      <VBox layoutX="-23.0" layoutY="45.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="7.0" prefWidth="600.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="28.0" prefWidth="50.0" stylesheets="@background.css" text="Back" />
                  <Button mnemonicParsing="false" stylesheets="@background.css" text="Cult Board" />
                  <Button mnemonicParsing="false" stylesheets="@background.css" text="Bonus Cards" />
                  <Button mnemonicParsing="false" stylesheets="@background.css" text="Town Tiles" />
                  <Button mnemonicParsing="false" stylesheets="@background.css" text="Round Tiles" />
                  <Button mnemonicParsing="false" stylesheets="@background.css" text="Victory Extensions" />
                  <Button fx:id="fullScreenButton" mnemonicParsing="false" onAction="#fullScreenButtonClicked" stylesheets="@background.css" text="Full Screen" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
